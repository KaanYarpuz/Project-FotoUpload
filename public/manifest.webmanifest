{
    "name": "Clicker",
    "short_name": "Clicker",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#FFFFFF",
    "lang": "en",
    "scope": "/",
    "description": "Clicker is een simplele foto app die je foto's laat maken en opslaan in je browser.",
    "prefer_related_applications": true,
    "orientation": "portrait",
    "theme_color": "#FFFFFF",
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "minimal-ui",
        "fullscreen",
        "browser"
    ],
    "icons": [
        {
            "src": "/images/icon.png",
            "sizes": "144x144",
            "type": "image/png"
        },
        {
            "src": "/images/icon.png",
            "sizes": "152x152",
            "type": "image/png"
        },
        {
            "src": "/images/icon.png",
            "sizes": "192x192",
            "type": "image/svg"
        },
        {
            "src": "/images/icon.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "launch_handler": {
        "client_mode": [
            "navigate-existing",
            "auto"
        ]
    }
}