{
  "name": "clickfick",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "nodemon": "nodemon ./bin/www",
    "watch": "npx tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/index.css --watch",
    "sync": "browser-sync start --proxy localhost:4000 --files=**/*  --ignore=node_modules ",
    "build": "npm-run-all -p start watch sync",
    "dev": "npm-run-all -p nodemon watch sync"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http-errors": "~2.0.0",
    "mongoose": "^8.1.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "tailwindcss": "^3.4.1"
  }
}
